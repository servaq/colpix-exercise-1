{
	"name": "colpix-exercise-1",
	"version": "1.0.0",
	"description": "COLPIX exercise 1",
	"main": "src/app.js",
	"scripts": {
		"test": "./node_modules/.bin/jasmine; exit 0",
		"transpile-dev": "./node_modules/.bin/babel --presets es2015 --plugins async-to-promises src --out-dir bin --source-maps inline",
		"transpile": "./node_modules/.bin/babel --presets es2015 --plugins async-to-promises src --out-dir bin",
		"create": "npm run transpile && ./node_modules/.bin/claudia create --region us-east-1 --api-module src/app --environment prod",
		"update": "npm run transpile && ./node_modules/.bin/claudia update",
		"server": "npm run transpile && ./node_modules/.bin/claudia-local-api --api-module bin/app.js"
	},
	"author": "Sergio Vaquero",
	"license": "ISC",
	"dependencies": {
		"claudia-api-builder": "^4.1.1",
		"knex": "^0.16.3",
		"mysql": "^2.16.0"
	},
	"devDependencies": {
		"@buckit/claudia-local-api": "git+https://github.com/BuckitApp/claudia-local-api.git",
		"babel-cli": "^6.26.0",
		"babel-plugin-async-to-promises": "^1.0.5",
		"babel-preset-es2015": "^6.24.1",
		"claudia": "^5.3.0",
		"jasmine": "^3.3.1"
	}
}
